{
    "$schema": "https://smapi.io/schemas/content-patcher.json",
    "Format": "2.5.0",
    "ConfigSchema": {
        "CopyQualitySetting": {
            "AllowValues": "disabled, enabled, auto",
            "Default": "auto",
            "Section": "Behavior"
        },
        "ShopEnabledSetting": {
            "AllowValues": "disabled, enabled",
            "Default": "enabled",
            "Section": "Map Changes"
        }
    },
    "DynamicTokens": [
        {
            "Name": "CopyQualityEnabled",
            "Value": false
        },
        {
            "Name": "MeadShopEnabled",
            "Value": false
        },
        {
            "Name": "MeadShopEnabled",
            "Value": true,
            "When": {
                "ShopEnabledSetting": "enabled"
            }
        },
        {
            "Name": "CopyQualityEnabled",
            "Value": true,
            "When": {
                "CopyQualitySetting": "enabled"
            }
        },
        {
            "Name": "CopyQualityEnabled",
            "Value": true,
            "When": {
                "CopyQualitySetting": "auto",
                "HasMod": "iakona1983.MachinesCopyQuality"
            }
        },
        {
            "Name": "CopyQualityEnabled",
            "Value": true,
            "When": {
                "CopyQualitySetting": "auto",
                "HasMod": "6480.qualitygoods"
            }
        },
        {
            "Name": "CopyQualityEnabled",
            "Value": true,
            "When": {
                "CopyQualitySetting": "auto",
                "HasMod": "voiddreams.ArtisanGoodsKeepQuality"
            }
        }
    ],
    "Changes": [
        //
        //
        // Load Data Files
        //
        //
        {
            "LogName": "Load Data Files",
            "Action": "Include",
            "FromFile": "Data/strings.json, Data/machines.json, Data/buffs.json, Data/mead.json, Data/cider.json, Data/operations.json"
        },
        {
            "When": {
                "ShopEnabledSetting": "enabled"
            },
            "LogName": "Load Map Data Files",
            "Action": "Include",
            "FromFile": "Maps/Data/meadshop.json, Data/mail.json, Data/gifttastes.json, Maps/Data/garbage.json, Data/shops.json, Data/characters.json"
        },
        //
        //
        //  Load Assets
        //
        //
        {
            "Action": "Load",
            "Target": "bonghits.BearHoneyMead/Objects",
            "FromFile": "assets/bh_mead.png"
        },
        {
